{
  "name": "reserve-api",
  "version": "2.4.1",
  "description": "Real-time booking and reservation system powered by GraphQL, Redis, and AWS",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.ts --runInBand",
    "migrate": "typeorm migration:run -d src/config/database.ts",
    "migrate:generate": "typeorm migration:generate -d src/config/database.ts",
    "migrate:revert": "typeorm migration:revert -d src/config/database.ts",
    "seed": "ts-node src/seeds/run.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [
    "graphql",
    "booking",
    "reservation",
    "real-time",
    "redis",
    "apollo",
    "typescript"
  ],
  "author": "Nicolas Lumbert",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@graphql-tools/merge": "^9.0.3",
    "@graphql-tools/schema": "^10.0.3",
    "aws-sdk": "^2.1583.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.4.2",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.15.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^5.0.0",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
